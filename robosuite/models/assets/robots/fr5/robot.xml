<mujoco model="fr5">
    <actuator>
        <!-- Physical limits of the actuator. -->
        <motor joint="shoulder_pan_joint" name="shoulder_pan"/>
        <motor joint="shoulder_lift_joint" name="shoulder_lift"/>
        <motor joint="elbow_joint" name="elbow"/>
        <motor joint="wrist_1_joint" name="wrist_1"/>
        <motor joint="wrist_2_joint" name="wrist_2"/>
        <motor joint="wrist_3_joint" name="wrist_3"/>
    </actuator>
    <asset>
        <material name="Gray" specular="0.5" shininess="0.25" rgba="0.82 0.82 0.82 1"/>
        <material name="FlangeGray" specular="0.5" shininess="0.25" rgba="0.6 0.6 0.6 1"/>
        <!-- base material and mesh -->
        <mesh name="base_vis" file="obj_meshes/base_vis/base_vis.obj"/>
        <!-- forearm mesh -->
        <mesh name="forearm_vis" file="obj_meshes/forearm_vis/forearm_vis.obj"/>
        <!-- shoulder material and mesh -->
        <mesh name="shoulder_vis" file="obj_meshes/shoulder_vis/shoulder_vis.obj"/>
        <!-- upperarm material and mesh -->
        <mesh name="upperarm_vis" file="obj_meshes/upperarm_vis/upperarm_vis.obj"/>
        <!-- wrist1 material and mesh -->
        <mesh name="wrist1_vis" file="obj_meshes/wrist1_vis/wrist1_vis.obj"/>
        <!-- wrist2 material and mesh -->
        <mesh name="wrist2_vis" file="obj_meshes/wrist2_vis/wrist2_vis.obj"/>
        <!-- wrist3 material and mesh -->
        <mesh name="wrist3_vis" file="obj_meshes/wrist3_vis/wrist3_vis.obj"/>

        <mesh name="base" file="meshes/base.stl" />
        <mesh name="shoulder" file="meshes/shoulder.stl" />
        <mesh name="upperarm" file="meshes/upperarm.stl" />
        <mesh name="forearm" file="meshes/forearm.stl" />
        <mesh name="wrist1" file="meshes/wrist1.stl" />
        <mesh name="wrist2" file="meshes/wrist2.stl" />
        <mesh name="wrist3" file="meshes/wrist3.stl" />
    </asset>

    <worldbody>
        <body name="base" quat="1 0 0 -1">
            <!-- robot view -->
            <camera mode="fixed" name="robotview" pos="1.0 0 0.4" quat="0.653 0.271 0.271 0.653"/>
            <inertial mass="1.6185" pos="0.00031896 0.00029673 0.042463" diaginertia="0.0030836 0.0030825 0.0045209" />
            <!-- mount attached here -->
            <geom mesh="base_vis" material="Gray" type="mesh" contype="0" conaffinity="0" group="1"/>
            <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="base" name="base_col"/>
            <body name="shoulder_link" pos="0 0 0.152">
                <inertial mass="4.64" pos="-0.00019 -0.00226 -0.01828" diaginertia="0.0093839 0.0093245 0.0077422" />
                <joint name="shoulder_pan_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-3.14159 3.14159" damping="0.001" frictionloss="0.01" />
                <geom mesh="shoulder_vis" material="Gray" type="mesh" contype="0" conaffinity="0" group="1"/>
                <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="shoulder" name="shoulder_col"/>
                <body name="upper_arm_link" pos="0 0 0" quat="1 1 0 0">
                    <inertial mass="10.08" pos="-0.2125 0 0.1212" diaginertia="0.020605 0.43899 0.43546" />
                    <joint name="shoulder_lift_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-4.71239 1.5708" damping="0.001" frictionloss="0.01" />
                    <geom mesh="upperarm_vis" material="Gray" type="mesh" contype="0" conaffinity="0" group="1"/>
                    <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="upperarm" name="upperarm_col"/>
                    <body name="forearm_link" pos="-0.425 0 0">
                        <inertial mass="2.71" pos="-0.27238 0.00017 0.01259" diaginertia="0.0035754 0.071483 0.071119" />
                        <joint name="elbow_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-3.14159 3.14159" damping="0.001" frictionloss="0.01" />
                        <geom mesh="forearm_vis" material="Gray" type="mesh" contype="0" conaffinity="0" group="1"/>
                        <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="forearm" name="forearm_col"/>
                        <body name="wrist_1_link" pos="-0.395 0 0.102">
                            <inertial mass="1.56" pos="0 -0.01468 -0.00233" diaginertia="0.001626 0.0012553 0.0015802" />
                            <joint name="wrist_1_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-4.71239 1.5708" damping="0.001" frictionloss="0.01" />
                            <geom mesh="wrist1_vis" material="Gray" type="mesh" contype="0" conaffinity="0" group="1"/>
                            <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="wrist1" name="wrist1_col"/>
                            <body name="wrist_2_link" pos="0 -0.102 0" quat="1 1 0 0">
                                <inertial mass="1.56" pos="0 0.01468 -0.00233" diaginertia="0.001626 0.0012553 0.0015802" />
                                <joint name="wrist_2_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-3.14159 3.14159" damping="0.001" frictionloss="0.01" />
                                <geom mesh="wrist2_vis" material="Gray" type="mesh" contype="0" conaffinity="0" group="1"/>
                                <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="wrist2" name="wrist2_col"/>
                                <body name="wrist_3_link" pos="0 0.1 0" quat="1 -1 0 0">
                                    <inertial mass="0.36" pos="0.00093 0.00081 -0.02005" diaginertia="0.00023009 0.00023049 0.0003593" />
                                    <joint name="wrist_3_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-3.14159 3.14159" damping="0.001" frictionloss="0.01" />
                                    <geom mesh="wrist3_vis" material="FlangeGray" type="mesh" contype="0" conaffinity="0" group="1"/>
                                    <geom type="mesh" rgba="0.7 0.7 0.7 1" mesh="wrist3" name="wrist3_col"/>
                                    <body name="right_hand" pos="0 0 0" >
                                        <!-- This camera points out from the eef. -->
                                        <camera mode="fixed" name="eye_in_hand" pos="0.05 0 0" quat="0 0.707108 0.707108 0" fovy="75"/>
                                        <!-- To add gripper -->
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
</mujoco>